<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="634.0" prefWidth="620.0" xmlns="http://javafx.com/javafx/20.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.jatelobank.Settings.Accounts.OtherAccount">
   <VBox layoutX="1.0" prefHeight="634.0" prefWidth="620.0" AnchorPane.rightAnchor="0.0">
      <Label style="-fx-font-size: 1.2em;" text="Beneficiary">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <TextField fx:id="accNoToBeSent" prefHeight="40.0" prefWidth="604.0" promptText="1100 XXXX XXXX XXXX*" style="-fx-font-size: 1.2em;">
         <VBox.margin>
            <Insets right="5.0" top="20.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="amount" layoutX="10.0" layoutY="29.0" prefHeight="40.0" promptText="Amount*" style="-fx-font-size: 1.2em;">
         <VBox.margin>
            <Insets right="5.0" top="20.0" />
         </VBox.margin>
      </TextField>
      <DatePicker fx:id="dateButton" editable="false" prefHeight="40.0" prefWidth="637.0" promptText="Date*" style="-fx-background-color: #132A12;">
         <VBox.margin>
            <Insets right="5.0" top="20.0" />
         </VBox.margin>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </DatePicker>
      <Label style="-fx-font-size: 1.2em;" text="Other data">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Label>
      <HBox prefHeight="75.0" prefWidth="523.0">
         <VBox.margin>
            <Insets right="5.0" />
         </VBox.margin>
         <TextField fx:id="transferPurpose" prefHeight="40.0" prefWidth="311.0" promptText="Transfer purpose" style="-fx-font-size: 1.2em;">
            <HBox.margin>
               <Insets right="10.0" top="20.0" />
            </HBox.margin>
         </TextField>
         <TextField fx:id="beneficiaryName" layoutX="10.0" layoutY="10.0" prefHeight="40.0" prefWidth="311.0" promptText="Beneficiary's name" style="-fx-font-size: 1.2em;">
            <HBox.margin>
               <Insets top="20.0" />
            </HBox.margin>
         </TextField>
      </HBox>
      <HBox layoutX="20.0" layoutY="217.0" prefHeight="75.0" prefWidth="538.0">
         <VBox.margin>
            <Insets right="5.0" />
         </VBox.margin>
         <TextField fx:id="beneficiaryEmail" prefHeight="40.0" prefWidth="311.0" promptText="Beneficiary's email" style="-fx-font-size: 1.2em;">
            <HBox.margin>
               <Insets right="10.0" top="20.0" />
            </HBox.margin>
         </TextField>
         <TextField fx:id="payerReference" layoutX="10.0" layoutY="10.0" prefHeight="40.0" prefWidth="311.0" promptText="Payer's reference" style="-fx-font-size: 1.2em;">
            <HBox.margin>
               <Insets top="20.0" />
            </HBox.margin>
         </TextField>
      </HBox>
      <TextArea fx:id="informationToBeneficiary" prefHeight="166.0" prefWidth="635.0" promptText="Information to beneficiary" style="-fx-font-size: 1.2em;">
         <VBox.margin>
            <Insets bottom="5.0" right="5.0" />
         </VBox.margin>
      </TextArea>
      <Label fx:id="label" style="-fx-text-fill: red; -fx-font-size: 1.2em;">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="5.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="continueButton" mnemonicParsing="false" onAction="#continueButt" prefHeight="40.0" prefWidth="643.0" style="-fx-text-fill: white; -fx-background-color: #132A12; -fx-font-size: 1.5em; -fx-font-weight: bold;" text="Transfer">
         <VBox.margin>
            <Insets right="5.0" />
         </VBox.margin>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </VBox>
</AnchorPane>
